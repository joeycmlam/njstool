{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "App-Diff-V2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/app/app-diff-v2/app.diff.v2.py",
            "console": "integratedTerminal",
            "args": [
                "--config",
                "${workspaceFolder}/src/app/app-diff-v2/config/config.json"
            ],
            "cwd": "${workspaceFolder}/src/app/app-diff-v2",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src/app/app-diff-v2"
            }
        },
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        }
    ]
}